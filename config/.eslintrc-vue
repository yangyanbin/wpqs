{
    "extends": ["eslint:recommended","plugin:vue/essential"],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules":{
        "no-debugger": 1,
        "no-console": 1,
        "no-mixed-spaces-and-tabs": 0
    }
}